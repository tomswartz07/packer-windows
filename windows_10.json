{
  "builders": [
    {
      "type": "virtualbox-iso",
      "iso_urls": [
        "./iso/Windows 10 ver. 1511.iso",
        "./iso/Windows 10 English x 64 full.iso",
        "./iso/Win10Eval.iso",
        "./iso/Windows10.iso",
        "http://go.microsoft.com/fwlink/p/?LinkId=522144",
        "./iso/Windows10_InsiderPreview_x64_EN-US_10074.iso"
      ],
      "iso_checksum_type": "none",
      "headless": false,
      "boot_wait": "2m",
      "boot_command": "<enter>",
      "ssh_username": "admin",
      "ssh_password": "admin",
      "ssh_wait_timeout": "8h",
      "shutdown_command": "shutdown /s /t 10 /f /d p:4:1 /c \"Packer build complete.\"",
      "guest_os_type": "Windows10_64",
      "vm_name": "Win10Packer",
      "guest_additions_mode": "disable",
      "disk_size": 61440,
      "floppy_files": [
        "./answer_files/10/Autounattend.xml",
        "./answer_files/sysprep/unattendSysprep.xml",
        "./scripts/openssh.ps1",
        "./scripts/windowsApps.ps1",
        "./scripts/fixnetwork.ps1",
        "./scripts/microsoft-updates.bat",
        "./scripts/dis-updates.ps1",
        "./scripts/win-updates.ps1",
        "./scripts/oracle-cert.cer"
      ],
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "2048"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "4"
        ]
      ]
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "remote_path": "/tmp/script.bat",
      "execute_command": "{{.Vars}} cmd /c C:/Windows/Temp/script.bat",
      "only": ["virtualbox-iso"],
      "scripts": [
        "./scripts/fixwin10.bat"
      ]
    },
    {
      "type": "windows-restart",
      "restart_timeout": "5m"
    },
    {
      "type": "shell",
      "remote_path": "/tmp/script.bat",
      "execute_command": "{{.Vars}} cmd /c C:/Windows/Temp/script.bat",
      "scripts": [
        "./scripts/pennmanorAD.bat"
      ]
    },
    {
      "type": "shell",
      "remote_path": "/tmp/script.bat",
      "execute_command": "{{.Vars}} cmd /c C:/Windows/Temp/script.bat",
      "scripts": [
        "./scripts/updates.bat"
      ],
      "pause_before": "5m"
    },
    {
      "type": "windows-restart",
      "restart_timeout": "1h"
    },
    {
      "type": "shell",
      "remote_path": "/tmp/script.bat",
      "execute_command": "{{.Vars}} cmd /c C:/Windows/Temp/script.bat",
      "scripts": [
        "./scripts/disable-auto-logon.bat",
        "./scripts/enable-rdp.bat",
        "./scripts/compile-dotnet-assemblies.bat",
        "./scripts/compact.bat",
        "./scripts/sysprep.bat"
      ],
      "pause_before": "5m"
    }
  ],
  "post-processors": [
    {
      "type": "compress",
      "output": "windows_81.tar.gz"
    }
  ]
}
